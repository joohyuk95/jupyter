   "source": [
    "import cv2\n",
    "\n",
    "img = cv2.imread('./data/lena.jpg')\n",
    "\n",
    "img1 = cv2.flip(img, 1)\n",
    "img2 = cv2.flip(img, 0)\n",
    "img3 = cv2.flip(img1, 0)\n",
    "\n",
    "cv2.imshow('source', img)\n",
    "cv2.imshow('left and right',img1)\n",
    "cv2.imshow('up and down',img2)\n",
    "cv2.imshow('LR and UD',img3)\n",
    "\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  }
